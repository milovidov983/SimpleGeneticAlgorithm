  "version": "0.2.0",
  "configurations": [

    {
      "name": ".NET Core Launch (console)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      /*"program": "${workspaceFolder}/Examples/AutoTilingExample/bin/Debug/netcoreapp3.0/AutoTilingExample",*/
      /*"program": "${workspaceFolder}/Examples/Coroutine/bin/Debug/netcoreapp3.0/Couroutine",*/
      /*"program": "${workspaceFolder}/Examples/EditTextBox/bin/Debug/netcoreapp3.0/EditTextBox",*/
      "program": "${workspaceFolder}/Examples/FlippyFlop/bin/Debug/netcoreapp3.0/FlippyFlop",
      /*"program": "${workspaceFolder}/Examples/GridCollider/bin/Debug/netcoreapp3.0/GridCollider",*/
