      /*"program": "${workspaceFolder}/Examples/PlatformingMovement/bin/Debug/netcoreapp3.0/PlatformingMovement",*/
      "args": [],
      "cwd": "${workspaceFolder}",
      "console": "internalConsole",
      "stopAtEntry": false
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach",
      "processId": "${command:pickProcess}"
    }
  ]
